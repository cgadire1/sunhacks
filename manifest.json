{
   "manifest_version": 2,
    "name": "Adobe Express Music Importer",
    "version": "1.0",
    "description": "Transfer music files or links from Spotify or YouTube Music to Adobe Express.",
    "permissions": ["activeTab", "storage", "tabs"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png"
      }
    },
    "icons": {
      "16": "icon16.png"
    }
  }
  